{
  "name": "chirp",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "mazurevich",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prune": "turbo prune",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start --parallel",
    "test": "turbo run test",
    "test:staged": "turbo run test:staged -- ",
    "test:search": "turbo run test --filter=onehub-search",
    "start:admin": "turbo run start --filter=admin",
    "start:onehub": "turbo run start --filter=onehub-2.0",
    "start:onehub-dev1": "turbo run start-dev1 --filter=onehub-2.0",
    "start:admin-dev1": "turbo run start-dev1 --filter=admin",
    "start:search": "turbo run start --filter=onehub-search",
    "build:admin": "turbo run build --filter=admin",
    "build:onehub": "turbo run build --filter=onehub-2.0",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck --parallel",
    "format": "prettier --write \"**/*.{css,scss,js,jsx,ts,tsx,md}\"",
    "check:deps": "turbo run check:deps --parallel"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.1.0",
  "dependencies": {
    "cross-env": "7.0.3",
    "prettier": "2.6.2",
    "turbo": "1.8.5"
  }
}
